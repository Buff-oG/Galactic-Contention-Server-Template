// Galactic Contention snippet json configuration file about the mapvote plugin.
// Galactic Contention snippet json configuration file about the mapvote plugin.
// We are filtering Kamino, Legoes, Special, GLOPS and TDM layers from the map vote rotation to let the true GC experience of every layer available.

{
    "plugin": "MapVote",
    "enabled": true,
    "discordClient": "discord",
    "entriesAmount": 7,
    "automaticVoteStart": true,
    "minPlayersForVote": 30,
    "voteWaitTimeFromMatchStart": 10,
    "voteBroadcastInterval": 15,
    "numberRecentMapsToExlude": 6,
    "automaticSeedingMode": false,
    "gamemodeWhitelist": [ "INVASION" ],
    "layerLevelBlacklist": [
      "/GC_.*_Special/",
      "/Lego/",
      "/GLOPS/",
      "/GC_KaminoTraining/",
      "/TDM/"
    ],
    "layerLevelWhitelist": [],
    "applyBlacklistToWhitelist": true,
    "factionsBlacklist": [],
    "logToDiscord": true,
    "channelID": "1122233344455566778899",
    "persistentDataFile": "",
    "timezone": 2,
    "filterByMod": ["GC"],
    "minGamemodeEntries": {
        "raas": 1,
        "aas": 1,
        "invasion": 4,
        "insurgency": 1
            }
    },
    {
      "plugin": "SocketIOAPI",
      "enabled": true,
      "websocketPort": "$YOURPORT",
      "securityToken": "$YOURRANDOMTOKEN"
    },
